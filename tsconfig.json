{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/exam/detail/[examId].tsx",
    "app/exam/list/[partId].tsx",
    "app/_layout.tsx",
    "app/vocabulary/index.tsx",
    "app/vocabulary/_layout.tsx",
    "app/vocabulary/detail/[id].tsx",
    "app/grammar/_layout.tsx",
    "app/(tabs)/_layout.tsx",
    "app/exam/prepare/[id].tsx",
    "app/onboarding.tsx",
    "app/index.tsx",
    "app/(tabs)/mock-test.jsx",
    "app/(tabs)/chatAI.tsx",
    "context/AuthContext.jsx",
    "context/AuthContext.jsx",
    "context/AuthContext.jsx",
    "constants/colors.jsx",
    "components/QuestionCard.jsx",
    "components/NavigationButtons.jsx",
    "components/Modal.jsx",
    "components/HeaderCard.jsx",
    "components/Header.jsx",
    "components/ExamList.jsx",
    "components/AudioControls.tsx",
    "components/Home/Header.jsx",
    "app/(tabs)/chatAI.tsx",
    "context/AuthContext.jsx",
    "context/AuthContext.jsx",
    "context/UserDetailContext.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/mock-test.jsx",
    "app/(tabs)/profile.jsx",
    "app/(tabs)/study-plan.jsx",
    "app/splash.jsx",
    "app/auth/login.js"
  ],
  "extends": "expo/tsconfig.base",
  "ts-node": {
    "esm": false
  }
}